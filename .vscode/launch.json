  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "ClrAppApplication",
      "request": "launch",
      "mainClass": "com.frankmoley.lil.clr_app.ClrAppApplication",
      "projectName": "clr-app"
    },
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "BootingApplication",
      "request": "launch",
      "mainClass": "com.frankmoley.lil.booting.BootingApplication",
      "projectName": "booting",
      "args": "--spring.profiles.active=prod"
    }
  ]
}